<h3><%= title %></h3>
<form action="/users/create" method="POST" >
        <label for="email">Email</label><br>
        <input type="email" id="email" name="email"  placeholder="PLease Entre Your Email" required >
        <hr>
        <label for="password">PassWord</label><br>
        <input type="password" id="password" name="password"  placeholder="PassWord" required ><br>
        <input type="password" id="password" name="confirm_password"  placeholder=" Confirm PassWord" required >
        <hr>
        <label for="name">Name</label><br>
        <input type="text" id="name" name="name"  placeholder="PLease Entre Your Name" required >

        <input type="submit" value="SIGN-UP">
</form>

